{
	"name": "Phillow Dark",
	"type": "dark",
	"colors": {
    "activityBar.background": "#19181A",
		"activityBarBadge.background": "#4caf50",
    "activityBarBadge.foreground": "#19181A",
    "activityBar.foreground": "#C1C0C0",
    "editor.background": "#252526",
		"editor.foreground": "#b3b3b3",
    "panel.background": "#403E41",
    "panelSectionHeader.foreground": "#ffffff",
    "sideBar.background": "#323232",
    "sideBar.foreground": "#b3b3b3",
		"sideBarTitle.foreground": "#2d2d2d",
    "statusBar.background" : "#2d2d2d",
    "statusBar.noFolderBackground" : "#2d2d2d",
    "tab.activeBorder": "#4caf50"
	},
	"tokenColors": [
    // HTML
    {
      "scope": "entity.name.tag.html",
      "name": "Tags",
      "settings": {
        "foreground": "#6e9aff"
      }
    },
    {
      "scope": "entity.other.attribute-name.html",
      "name": "Attribute",
      "settings": {
        "foreground": "#d16099",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation.separator.key-value.html, punctuation.definition.string.begin.html, punctuation.definition.string.end.html",
      "name": "Seperator",
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "scope": "meta.attribute.id.html, meta.attribute.class.html",
      "name": "Class/ID",
      "settings": {
        "foreground": "#FFD866",
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation.definition.comment.html, comment.block.html",
      "name": "Comment",
      "settings": {
        "foreground": "#777777",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "meta.tag.structure.div.start.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html",
      "name": "Other",
      "settings": {
        "foreground": "#6e9aff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "text.html.derivative",
      "name": "Body Text",
      "settings": {
        "foreground": "#efefef"
      }
    },
    {
      "scope": "meta.attribute.href.html",
      "name": "HREF",
      "settings": {
        "foreground": "#6e62d8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "invalid.deprecated.entity.other.attribute-name.html",
      "name": "Invalid",
      "settings": {
        "foreground": "#c23a3a"
      }
    },
    {
      "scope": "string.quoted.double.html",
      "name": "",
      "settings": {
        "foreground": "#9e9e9e"
      }
    },
    // JavaScript || TypeScript || JSX
    {
      "scope": "source.js",
      "name": "Function",
      "settings": {
        "foreground": "#efefef"
      }
    },
    {
      "scope": "keyword.control.loop.js",
      "name": "",
      "settings": {
        "foreground": "#6e9aff"
      }
    },
    {
      "scope": "entity.name.tag.js, punctuation.definition.tag.begin.js, punctuation.definition.tag.end.js",
      "name": "Tag",
      "settings": {
        "foreground": "#6e9aff"
      }
    },
    {
      "scope": "punctuation.definition.tag.begin.js, punctuation.definition.tag.end.js",
      "name": "Tag",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable.other.object.js",
      "name": "Variable",
      "settings": {
        "foreground": "#9e9e9e"
      }
    },
    {
      "scope": "keyword.operator.logical.js",
      "name": "Other",
      "settings": {
        "foreground": "#FC9867"
      }
    },
    {
      "scope": "variable.other.property.js",
      "name": "Other",
      "settings": {
        "foreground": "#78DCE8"
      }
    },
  ]
}