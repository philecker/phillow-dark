{
  "name": "Phillow Dark",
  "type": "dark",
  "colors": {
    "activityBar.background": "#181818",
    "activityBarBadge.background": "#4caf50",
    "activityBarBadge.foreground": "#252526",
    "activityBar.foreground": "#C1C0C0",
    "badge.background": "#4caf50",
    "editor.background": "#202020",
    "editor.foreground": "#b3b3b3",
    "editorError.foreground": "#da6a48",
    "editor.wordHighlightBackground": "#2D2D2D",
    "editor.findMatchBackground": "#2D2D2D",
    "editor.findMatchHighlightBackground": "#2D2D2D",
    "editor.findMatchBorder": "#2D2D2D",
    "editor.findMatchHighlightBorder": "#2D2D2D",
    "editor.findRangeHighlightBorder": "#2D2D2D",
    "editor.selectionBackground": "#2D2D2D",
    "editor.inactiveSelectionBackground": "#2D2D2D",
    // "gitDecoration.modifiedResourceForeground": "#4caf50",
    "input.foreground": "#CCCCCC",
    "input.placeholderForeground": "#CCCCCC",
    "list.activeSelectionBackground": "#252526",
    "list.focusOutline": "#252526",
    "panel.border": "#2D2D2D",
    "panel.background": "#1C1C1C",
    "panelSectionHeader.background": "#5f5f5f",
    "panelSectionHeader.foreground": "#cccccc",
    "panelTitle.inactiveForeground": "#999999",
    "settings.numberInputForeground": "#CCCCCC",
    "sideBar.background": "#2D2D2D",
    "sideBar.foreground": "#b3b3b3",
    "sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#4C4C4C",
    "statusBar.background": "#4C4C4C",
    "statusBar.noFolderBackground": "#4C4C4C",
    "statusBarItem.prominentBackground": "#159c93",
    "statusBar.border": "#2D2D2D",
    "tab.activeBorderTop": "#4caf50",
    "terminal.background":"#1C1C1C",
    "terminal.foreground":"#CCCCCC",
    "terminalCursor.background":"#aca59c",
    "terminalCursor.foreground":"#aca59c",
    "terminal.ansiBlack": "#282a36",
    "terminal.ansiRed": "#ff5c57",
    "terminal.ansiGreen": "#5af78e",
    "terminal.ansiYellow": "#f3f99d",
    "terminal.ansiBlue": "#57c7ff",
    "terminal.ansiMagenta": "#ff6ac1",
    "terminal.ansiCyan": "#9aedfe",
    "terminal.ansiWhite": "#f1f1f0",
    "terminal.ansiBrightBlack": "#686868",
    "terminal.ansiBrightRed": "#ff5c57",
    "terminal.ansiBrightGreen": "#5af78e",
    "terminal.ansiBrightYellow": "#f3f99d",
    "terminal.ansiBrightBlue": "#57c7ff",
    "terminal.ansiBrightMagenta": "#ff6ac1",
    "terminal.ansiBrightCyan": "#9aedfe",
    "terminal.ansiBrightWhite": "#eff0eb"
  },
  "tokenColors": [
    {
      "scope": "entity.name.function, meta.object-literal.key",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.modifier, variable.language.this, storage.type, entity.other.attribute-name, punctuation.definition.comment, comment.block, keyword.control.default, meta.object-literal.key",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.template.js.jsx",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "",
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": "entity.name.tag, support.type.property-name, entity.name.function, punctuation.definition.heading.markdown, punctuation.definition.comment.dockerfile",
      "settings": {
        "foreground": "#5186e9"
      }
    },
    {
      "scope": "entity.other.attribute-name, keyword.operator.assignment, punctuation.separator, meta.tag.attributes, meta.link.inline.markdown",
      "settings": {
        "foreground": "#c279cf"
      }
    },
    {
      "scope": "meta.tag.attributes.js.jsx, constant.language.boolean, constant.language.null.js, meta.tag.without-attributes.js.jsx,",
      "settings": {
        "foreground": "#e8f5e9"
      }
    },
    {
      "scope": "string.quoted.single, string.quoted.double, variable.other.object.property, variable.other.property",
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      "scope": "comment.block.html, comment.line.double-slash.js, punctuation.definition.comment, comment.block, punctuation.definition, punctuation.support, variable.other.readwrite, punctuation.support.type.property-name.begin.json, punctuation.definition.comment.js.jsx, comment.line.double-slash.js.jsx, punctuation.definition.block.tag.jsdoc",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "scope": "keyword.other.special-method.dockerfile, support.constant.property-value.css, keyword.other.using.cs, meta.import, storage.type.function.arrow, keyword.operator.comparison, markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#da6a48"
      }
    },
    {
      "scope": "support.type.property-name.css, variable.parameter, meta.object-literal.key, meta.object-literal.key, support.type.property-name.json, keyword.other.get.cs, keyword.other.set.cs",
      "settings": {
        "foreground": "#ac82f5"
      }
    },
    {
      "scope": "punctuation.definition.block, punctuation.section.embedded.begin, punctuation.section.embedded.begin.js, support.variable.property",
      "settings": {
        "foreground": "#ffe082"
      }
    },
    {
      "scope": "punctuation.definition.variable.less, variable.other.less, variable.scss, keyword.type, entity.name.type.namespace, keyword.control.flow, variable.other.readwrite.alias, variable.other.object, variable.other.readwrite, variable.other.constant",
      "settings": {
        "foreground": "#51c7b3"
      }
    },
    {
      "scope": "punctuation.definition.keyword.scss, keyword.control.at-rule.mixin.scss, keyword.control.at-rule.include.scss, support.constant.handlebars, storage.modifier, variable.language.this, storage.type",
      "settings": {
        "foreground": "#92c452"
      }
    }
  ]
}