{
	"name": "Phillow Dark",
	"type": "dark",
	"colors": {
    "activityBar.background": "#19181A",
		"activityBarBadge.background": "#4caf50",
    "activityBarBadge.foreground": "#19181A",
    "activityBar.foreground": "#C1C0C0",
    "editor.background": "#252526",
		"editor.foreground": "#b3b3b3",
    "panel.background": "#403E41",
    "panelSectionHeader.foreground": "#ffffff",
    "panelTitle.inactiveForeground": "#999999",
    "panel.border": "#323232",
    "sideBar.background": "#323232",
    "sideBar.foreground": "#b3b3b3",
		"sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#4C4C4C",
    "statusBar.background": "#4C4C4C",
    "statusBar.noFolderBackground": "#4C4C4C",
    "statusBarItem.prominentBackground": "#159c93",
    "statusBar.border": "#323232",
    "tab.activeBorder": "#4caf50",
    "widget.shadow": "#19181A"
	},
	"tokenColors": [
    {
      "scope": "entity.name.function.js, meta.object-literal.key.js.jsx",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable.language.this.js.jsx, storage.type, entity.other.attribute-name.html, entity.other.attribute-name.js, punctuation.definition.comment, comment.block, keyword.control.default, meta.object-literal.key.js.jsx",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.template.js.jsx",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "",
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": "entity.name.tag, support.type.property-name, entity.name.function",
      "settings": {
        "foreground": "#4277da"
      }
    },
    {
      "scope": "entity.other.attribute-name, keyword.operator.assignment, punctuation.separator, meta.tag.attributes",
      "settings": {
        "foreground": "#c279cf"
      }
    },
    {
      "scope": "meta.tag.attributes.js.jsx, constant.language.boolean, constant.language.null.js, variable.other.readwrite.js, meta.tag.without-attributes.js.jsx",
      "settings": {
        "foreground": "#e8f5e9"
      }
    },
    {
      "scope": "comment.block.html, comment.line.double-slash.js, punctuation.definition.comment, comment.block, punctuation.definition, punctuation.support, variable.other.readwrite, punctuation.support.type.property-name.begin.json, punctuation.definition.comment.js.jsx, comment.line.double-slash.js.jsx, punctuation.definition.block.tag.jsdoc",
      "settings": {
        "foreground": "#616161"
      }
    },
    {
      "scope": "meta.import, storage.type.function.arrow, keyword.operator.comparison",
      "settings": {
        "foreground": "#e7967d"
      }
    },
    {
      "scope": "variable.language.this.js.jsx, storage.type",
      "settings": {
        "foreground": "#b43939"
      }
    },
    {
      "scope": "meta.object-literal.key.js.jsx, meta.object-literal.key",
      "settings": {
        "foreground": "#8456d4"
      }
    },
    {
      "scope": "punctuation.definition.block, punctuation.section.embedded.begin, punctuation.section.embedded.begin.js",
      "settings": {
        "foreground": "#ffe082"
      }
    },
    {
      "scope": "keyword.control.flow, variable.other.readwrite.alias.js, variable.other.constant.js, support.type.property-name.json",
      "settings": {
        "foreground": "#51c7b3"
      }
    },
  ]
}