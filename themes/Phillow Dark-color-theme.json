{
	"name": "Phillow Dark",
	"type": "dark",
	"colors": {
    "activityBar.background": "#19181A",
		"activityBarBadge.background": "#4caf50",
    "activityBarBadge.foreground": "#19181A",
    "activityBar.foreground": "#C1C0C0",
    "editor.background": "#252526",
		"editor.foreground": "#b3b3b3",
    "panel.background": "#403E41",
    "panelSectionHeader.foreground": "#ffffff",
    "sideBar.background": "#323232",
    "sideBar.foreground": "#b3b3b3",
		"sideBarTitle.foreground": "#2d2d2d",
    "statusBar.background" : "#2e7d32",
    "statusBar.noFolderBackground" : "#2e7d32",
    "tab.activeBorder": "#4caf50"
	},
	"tokenColors": [
    {
      "scope": "entity.name.tag, keyword.control.conditional, meta.embedded.expression",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.attribute-name.html, punctuation.definition.comment, comment.block, storage.type, keyword.control.default",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.tag, support.type.property-name, entity.name.function",
      "settings": {
        "foreground": "#4277da"
      }
    },
    {
      "scope": "entity.other.attribute-name, keyword.operator.assignment, punctuation.separator, meta.tag.attributes",
      "settings": {
        "foreground": "#c279cf"
      }
    },
    {
      "scope": "string.quoted.double, string.quoted.single",
      "settings": {
        "foreground": "#e8f5e9"
      }
    },
    {
      "scope": "punctuation.definition, punctuation.support, variable.other.readwrite",
      "settings": {
        "foreground": "#9e9e9e"
      }
    },
    {
      "scope": "punctuation.definition.comment, comment.block, storage.type",
      "settings": {
        "foreground": "#757575"
      }
    },
    {
      "scope": "storage.type.function.arrow, keyword.operator.comparison",
      "settings": {
        "foreground": "#ff8a65"
      }
    },
    {
      "scope": "meta.import",
      "settings": {
        "foreground": "#ac443e"
      }
    },
    {
      "scope": "constant.language.boolean, punctuation.definition.block, punctuation.section.embedded.begin",
      "settings": {
        "foreground": "#ffe082"
      }
    },
    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#50939B"
      }
    },
  ]
}