{
	"name": "Phillow Dark",
	"type": "dark",
	"colors": {
    "activityBar.background": "#1d1c1c",
		"activityBarBadge.background": "#4caf50",
    "activityBarBadge.foreground": "#1d1c1c",
    "activityBar.foreground": "#C1C0C0",
    "editor.background": "#252526",
		"editor.foreground": "#b3b3b3",
    "editor.wordHighlightBackground": "#323232",
    "editor.findMatchBackground": "#323232",
    "editor.findMatchHighlightBackground": "#323232",
    "editor.findMatchBorder": "#323232",
    "editor.findMatchHighlightBorder": "#323232",
    "editor.findRangeHighlightBorder": "#323232",
    "editor.selectionBackground": "#323232",
    "editor.inactiveSelectionBackground": "#323232",
    "settings.numberInputForeground": "#323232",
    "panel.background": "#403E41",
    "panelSectionHeader.foreground": "#ffffff",
    "panelTitle.inactiveForeground": "#999999",
    "panel.border": "#323232",
    "sideBar.background": "#323232",
    "sideBar.foreground": "#b3b3b3",
		"sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#4C4C4C",
    "statusBar.background": "#4C4C4C",
    "statusBar.noFolderBackground": "#4C4C4C",
    "statusBarItem.prominentBackground": "#159c93",
    "statusBar.border": "#323232",
    "tab.activeBorderTop": "#4caf50",
    "widget.shadow": "#19181A"
	},
	"tokenColors": [
    {
      "scope": "entity.name.function.js, meta.object-literal.key.js.jsx",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "storage.modifier, variable.language.this.js.jsx, storage.type, entity.other.attribute-name.html, entity.other.attribute-name.js, punctuation.definition.comment, comment.block, keyword.control.default, meta.object-literal.key.js.jsx",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.template.js.jsx",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "",
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": "entity.name.tag, support.type.property-name, entity.name.function, punctuation.definition.heading.markdown",
      "settings": {
        "foreground": "#5186e9"
      }
    },
    {
      "scope": "entity.other.attribute-name, keyword.operator.assignment, punctuation.separator, meta.tag.attributes, meta.link.inline.markdown",
      "settings": {
        "foreground": "#c279cf"
      }
    },
    {
      "scope": "meta.tag.attributes.js.jsx, constant.language.boolean, constant.language.null.js, meta.tag.without-attributes.js.jsx",
      "settings": {
        "foreground": "#e8f5e9"
      }
    },
    {
      "scope": "comment.block.html, comment.line.double-slash.js, punctuation.definition.comment, comment.block, punctuation.definition, punctuation.support, variable.other.readwrite, punctuation.support.type.property-name.begin.json, punctuation.definition.comment.js.jsx, comment.line.double-slash.js.jsx, punctuation.definition.block.tag.jsdoc",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "scope": "keyword.other.using.cs, meta.import, storage.type.function.arrow, keyword.operator.comparison, markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#da6a48"
      }
    },
    {
      "scope": "variable.parameter, meta.object-literal.key, meta.object-literal.key, support.type.property-name.json",
      "settings": {
        "foreground": "#ac82f5"
      }
    },
    {
      "scope": "punctuation.definition.block, punctuation.section.embedded.begin, punctuation.section.embedded.begin.js",
      "settings": {
        "foreground": "#ffe082"
      }
    },
    {
      "scope": "keyword.type, entity.name.type.namespace, keyword.control.flow, variable.other.readwrite.alias, variable.other.object, variable.other.readwrite, variable.other.constant",
      "settings": {
        "foreground": "#51c7b3"
      }
    },
    {
      "scope": "storage.modifier, variable.language.this.js.jsx, storage.type",
      "settings": {
        "foreground": "#92c452"
      }
    },
  ]
}