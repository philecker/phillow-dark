{
	"name": "Phillow Dark",
	"type": "dark",
	"colors": {
    "activityBar.background": "#19181A",
		"activityBarBadge.background": "#4caf50",
    "activityBarBadge.foreground": "#19181A",
    "activityBar.foreground": "#C1C0C0",
    "editor.background": "#252526",
		"editor.foreground": "#b3b3b3",
    "panel.background": "#403E41",
    "panelSectionHeader.foreground": "#ffffff",
    "sideBar.background": "#323232",
    "sideBar.foreground": "#b3b3b3",
		"sideBarTitle.foreground": "#2d2d2d",
    "statusBar.background" : "#2e7d32",
    "statusBar.noFolderBackground" : "#2e7d32",
    "tab.activeBorder": "#4caf50"
	},
	"tokenColors": [
    {
      "scope": "entity.name.function.js, meta.object-literal.key.js.jsx",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable.language.this.js.jsx, entity.other.attribute-name.html, punctuation.definition.comment, comment.block, storage.type, keyword.control.default, meta.object-literal.key.js.jsx",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.template.js.jsx",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.tag, support.type.property-name, entity.name.function",
      "settings": {
        "foreground": "#4277da"
      }
    },
    {
      "scope": "entity.other.attribute-name, keyword.operator.assignment, punctuation.separator, meta.tag.attributes",
      "settings": {
        "foreground": "#c279cf"
      }
    },
    {
      "scope": "string.quoted.double, string.quoted.single, variable.other.readwrite.js, meta.tag.without-attributes.js.jsx",
      "settings": {
        "foreground": "#e8f5e9"
      }
    },
    {
      "scope": "variable.language.this.js.jsx, punctuation.definition.comment, comment.block, storage.type, punctuation.definition, punctuation.support, variable.other.readwrite, punctuation.support.type.property-name.begin.json, punctuation.definition.comment.js.jsx, comment.line.double-slash.js.jsx, punctuation.definition.block.tag.jsdoc",
      "settings": {
        "foreground": "#616161"
      }
    },
    {
      "scope": "storage.type.function.arrow, keyword.operator.comparison",
      "settings": {
        "foreground": "#ff8a65"
      }
    },
    {
      "scope": "meta.import",
      "settings": {
        "foreground": "#ff5722"
      }
    },
    {
      "scope": "meta.object-literal.key.js.jsx",
      "settings": {
        "foreground": "#7d45df"
      }
    },
    {
      "scope": "source.js.jsx",
      "settings": {
        "foreground": "#ffb300"
      }
    },
    {
      "scope": "constant.language.boolean, punctuation.definition.block, punctuation.section.embedded.begin, punctuation.section.embedded.begin.js",
      "settings": {
        "foreground": "#ffe082"
      }
    },
    {
      "scope": "keyword.control.flow, variable.other.readwrite.alias.js, variable.other.constant.js, support.type.property-name.json",
      "settings": {
        "foreground": "#51c7b3"
      }
    },
  ]
}